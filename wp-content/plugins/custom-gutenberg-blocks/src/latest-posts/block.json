{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "create-block/latest-posts-section",
	"version": "0.1.0",
	"title": "Latest Posts Section",
	"category": "custom-blocks",
	"icon": "megaphone",
	"description": "Sektion um die neuesten Posts eines ausgewählten Types anzuzeigen. Mit optionalem Titel, Subline und Farbvariante.",
	"attributes": {
	  "headline": {
		"type": "string",
		"default": ""
	  },
	  "subheadline": {
		"type": "string",
		"default": ""
	  },
	  "postType": {
		"type": "string",
		"default": "post"
	  },
	  "variant": {
		"type": "string",
		"default": "light"
	  },
	  "buttonLabel": {
		"type": "string",
		"default": "Alle Beiträge"
	  }
	},
	"supports": {
	  "html": false
	},
	"textdomain": "custom-gutenberg-blocks",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php"
  }
