{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "create-block/cards-grid",
	"version": "0.1.0",
	"title": "Cards",
	"category": "widgets",
	"icon": "smiley",
	"description": "Section with up to 4 cards, each containing image/icon, title and description.",
	"attributes": {
	  "title": {
		"type": "string",
		"default": ""
	  },
	  "subtitle": {
		"type": "string",
		"default": ""
	  },
	  "cards": {
		"type": "array",
		"default": []
	  }
	},
	"example": {},
	"supports": {
	  "html": false
	},
	"textdomain": "custom-gutenberg-blocks",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php",
	"viewScript": "file:./view.js"
  }
