{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 3,
    "name": "custom-block/download-links",
    "version": "0.1.0",
    "title": "Download Links",
    "category": "layout",
    "icon": "download",
    "description": "A block for adding downloadable links with various variants.",
    "keywords": ["download", "links", "files", "buttons"],
    "attributes": {
        "title": {
            "type": "string",
            "default": "Download Links"
        },
        "variant": {
            "type": "string",
            "default": "light"
        },
        "links": {
            "type": "array",
            "default": []
        }
    },
    "supports": {
        "html": false,
        "align": ["wide", "full"]
    },
    "textdomain": "custom-gutenberg-blocks",
    "editorScript": "file:./index.js",
    "editorStyle": "file:./index.css",
    "style": "file:./style-index.css",
    "render": "file:./render.php"
}
